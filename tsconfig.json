{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    
  },
  "include": [
    "src/**/*", "src/main.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
